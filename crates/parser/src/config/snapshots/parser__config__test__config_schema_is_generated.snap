                    },
                    {
                      "title": "ISO-2022-JP",
                      "default": "ISO-2022-JP",
                      "const": "ISO-2022-JP"
                    },
                    {
                      "title": "Shift_JIS",
                      "default": "Shift_JIS",
                      "const": "Shift_JIS"
                    },
                    {
                      "title": "EUC-KR",
