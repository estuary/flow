                      "const": "ISO-8859-7",
                      "default": "ISO-8859-7"
                    },
                    {
                      "title": "ISO-8859-8",
                      "const": "ISO-8859-8",
                      "default": "ISO-8859-8"
                    },
                    {
                      "title": "ISO-8859-8-I",
                      "const": "ISO-8859-8-I",
                      "default": "ISO-8859-8-I"
                    },
                    {
                      "title": "ISO-8859-10",
                      "const": "ISO-8859-10",
                      "default": "ISO-8859-10"
                    },
                    {
                      "title": "ISO-8859-13",
                      "const": "ISO-8859-13",
                      "default": "ISO-8859-13"
                    },
                    {
                      "title": "ISO-8859-14",
                      "const": "ISO-8859-14",
                      "default": "ISO-8859-14"
                    },
                    {
                      "title": "ISO-8859-15",
                      "const": "ISO-8859-15",
                      "default": "ISO-8859-15"
                    },
                    {
                      "title": "ISO-8859-16",
                      "const": "ISO-8859-16",
                      "default": "ISO-8859-16"
                    },
                    {
                      "title": "KOI8-R",
                      "const": "KOI8-R",
                      "default": "KOI8-R"
                    },
                    {
                      "title": "KOI8-U",
                      "const": "KOI8-U",
                      "default": "KOI8-U"
                    },
                    {
                      "title": "macintosh",
                      "const": "macintosh",
                      "default": "macintosh"
                    },
                    {
                      "title": "windows-874",
                      "const": "windows-874",
                      "default": "windows-874"
                    },
                    {
                      "title": "windows-1250",
                      "const": "windows-1250",
                      "default": "windows-1250"
                    },
                    {
                      "title": "windows-1251",
                      "const": "windows-1251",
                      "default": "windows-1251"
                    },
                    {
                      "title": "windows-1252",
                      "const": "windows-1252",
                      "default": "windows-1252"
                    },
                    {
                      "title": "windows-1253",
                      "const": "windows-1253",
                      "default": "windows-1253"
                    },
                    {
                      "title": "windows-1254",
