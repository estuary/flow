                      "default": "windows-1255"
                    },
                    {
                      "title": "windows-1256",
                      "const": "windows-1256",
                      "default": "windows-1256"
                    },
                    {
                      "title": "windows-1257",
                      "const": "windows-1257",
                      "default": "windows-1257"
                    },
                    {
                      "title": "windows-1258",
                      "const": "windows-1258",
                      "default": "windows-1258"
                    },
                    {
                      "title": "x-mac-cyrillic",
                      "const": "x-mac-cyrillic",
                      "default": "x-mac-cyrillic"
                    },
                    {
                      "title": "GBK",
                      "const": "GBK",
                      "default": "GBK"
                    },
                    {
                      "title": "gb18030",
                      "const": "gb18030",
                      "default": "gb18030"
                    },
                    {
                      "title": "Big5",
                      "const": "Big5",
                      "default": "Big5"
                    },
                    {
                      "title": "EUC-JP",
                      "const": "EUC-JP",
                      "default": "EUC-JP"
                    },
                    {
                      "title": "ISO-2022-JP",
                      "const": "ISO-2022-JP",
                      "default": "ISO-2022-JP"
                    },
                    {
                      "title": "Shift_JIS",
                      "const": "Shift_JIS",
                      "default": "Shift_JIS"
                    },
                    {
                      "title": "EUC-KR",
                      "const": "EUC-KR",
                      "default": "EUC-KR"
                    },
                    {
                      "title": "Auto",
                      "const": null,
                      "default": null
                    }
                  ]
                },
                "errorThreshold": {
                  "title": "Error Threshold",
                  "description": "Allows a percentage of errors to be ignored without failing the entire\nparsing process. When this limit is exceeded, parsing halts.",
                  "type": "integer",
                  "default": 0,
                  "maximum": 100,
                  "minimum": 0
                },
                "escape": {
                  "title": "Escape Character",
                  "description": "The escape character, used to escape quotes within fields.",
                  "default": null,
                  "oneOf": [
                    {
                      "title": "Backslash (\\)",
                      "const": "\\",
                      "default": "\\"
                    },
                    {
                      "title": "Disable Escapes",
                      "const": "",
                      "default": ""
                    },
                    {
                      "title": "Auto",
                      "const": null,
                      "default": null
                    }
                  ]
                },
                "headers": {
                  "description": "Manually specified headers, which can be used in cases where the file itself doesn't\ncontain a header row. If specified, then the parser will assume that the first row is data,\nnot column names, and the column names given here will be used. The column names will be\nmatched with the columns in the file by the order in which they appear here.",
                  "type": "array",
                  "default": [],
                  "items": {
                    "type": "string"
                  }
                },
                "lineEnding": {
                  "title": "Line Ending",
                  "description": "The value that terminates a line. Only single-byte values are supported, with the\nexception of \"\\r\\n\" (CRLF), which will accept lines terminated by\neither a carriage return, a newline, or both.",
                  "default": null,
                  "oneOf": [
                    {
                      "title": "CRLF (\\r\\n) (Windows)",
                      "const": "\r\n",
                      "default": "\r\n"
                    },
                    {
                      "title": "CR (\\r)",
                      "const": "\r",
                      "default": "\r"
                    },
                    {
                      "title": "LF (\\n)",
                      "const": "\n",
                      "default": "\n"
                    },
                    {
                      "title": "Record Separator (0x1E)",
                      "const": "\u001e",
                      "default": "\u001e"
                    },
                    {
                      "title": "Auto",
                      "const": null,
                      "default": null
                    }
                  ]
                },
                "quote": {
                  "title": "Quote Character",
                  "description": "The character used to quote fields.",
                  "default": null,
                  "oneOf": [
                    {
                      "title": "Double Quote (\")",
                      "const": "\"",
                      "default": "\""
                    },
                    {
                      "title": "Single Quote (')",
                      "const": "'",
                      "default": "'"
                    },
                    {
                      "title": "Disable Quoting",
                      "const": "",
                      "default": ""
                    },
                    {
                      "title": "Auto",
                      "const": null,
                      "default": null
                    }
                  ]
                },
                "skipLines": {
                  "description": "Skip a number of lines at the beginning of the file before parsing begins.\nThis is useful for skipping over metadata that is sometimes added to the top of files.",
                  "type": "integer",
                  "format": "uint",
                  "minimum": 0
                }
              }
            },
            "type": {
              "type": "string",
              "const": "csv",
              "default": "csv"
            }
          },
          "required": [
            "type",
            "config"
          ]
        },
        {
          "title": "Protobuf",
          "description": "Parses a single protobuf message, using the given .proto file in the configuration.",
          "type": "object",
          "properties": {
            "config": {
              "type": "object",
              "properties": {
                "message": {
                  "description": "The name of the protobuf Message to deserialize as. Must be defined within the given proto\nfile.",
                  "type": "string"
                },
                "protoFile": {
                  "title": "Your .proto file",
                  "description": "The contents of the .proto that defines the message type to deserialize.",
                  "type": "string",
                  "multiline": true
                }
              },
              "required": [
                "protoFile",
                "message"
              ]
            },
            "type": {
              "type": "string",
              "const": "protobuf",
              "default": "protobuf"
            }
          },
          "required": [
            "type",
            "config"
          ]
        },
        {
          "title": "W3C Extended Log",
          "description": "A W3C Extended Log file, as defined by the working group draft at:\nhttps://www.w3.org/TR/WD-logfile.html",
