                    },
                    {
                      "title": "KOI8-R",
                      "default": "KOI8-R",
                      "const": "KOI8-R"
                    },
                    {
                      "title": "KOI8-U",
                      "default": "KOI8-U",
                      "const": "KOI8-U"
                    },
                    {
                      "title": "macintosh",
                      "default": "macintosh",
                      "const": "macintosh"
                    },
                    {
                      "title": "windows-874",
