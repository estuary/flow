{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "UserAccessFullBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::EXAMPLE_BUCKET"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject"
            ],
            "Resource": [
                "arn:aws:s3:::EXAMPLE_BUCKET/*"
            ]
        }
    ]
}