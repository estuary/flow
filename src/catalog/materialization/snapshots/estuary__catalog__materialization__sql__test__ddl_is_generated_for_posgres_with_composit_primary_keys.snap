	"numCol" BIGINT NOT NULL,
	"boolCol" BOOLEAN NOT NULL,
	"objCol" JSON NOT NULL,
	"arrayCol" JSON NOT NULL,
	"intColNullable" BIGINT,
	"numColNullable" DOUBLE PRECISION,
